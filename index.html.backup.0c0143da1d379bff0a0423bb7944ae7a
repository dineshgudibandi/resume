<script>
// Create a YUI sandbox on your page.
YUI().use('node', 'event','node-event-simulate', function (Y) {
  Y.one("*").on("click",function(e){
	  if(e.target.get('id')!="more-skills"){
		  Y.one('.navrail-more-menu-wrapper').addClass('d-n');
	  }
  });
  Y.one("#more-skills").on("click",function(e){
	  Y.one('.navrail-more-menu-wrapper').removeClass('d-n');
  });
  Y.all("#ctype li a").on("click",function(e){  
	   Y.all('#companies li').each(function (node) {
		  if (node.get("className").match(e.target.get("id"))) {
			 node.show();
		   }
		   else{
			   node.hide();
		   }
		});
  });
  var titles=new Array("American Express Merchant Site","Yahoo's Help site","Delta Shopping Cart","CanvasPress","Doctor is Online", "One and Us");
  var urls=new Array("https://www.americanexpress.com/merchant","http://help.yahoo.com","http://www.delta.com","http://www.canvaspress.com","http://www.drisonline.com","http://www.oneandus.com");
  var sentences=Array("American Express. also known as Amex,is an American multinational Financial Services Corporation headquartered in Manhattan's Three World Financial Center in New York City, United States","Yahoo Inc. is an American multinational Internet corporation headquartered in Sunnyvale, California. It is globally known for its Web portal, search engine Yahoo Search, and related services.","Delta Air Lines Inc, operating as Delta Air Lines, is a major American airline headquartered in Atlanta, Georgia, United States.","Canvaspress is one of the leading canvas printing companies in the United States. They print photos on to canvas, aluminum and wall papers. They are headquartered in Round Rock, TX.","Phycor is an innovative virtual doctor visit service provider in health care services.They changed the way how the doctor visits happen in today’s world. They are located in Houston, TX.","North Star Global is one of the prominent calling card service retailers in the United States. They are located in Atlanta, GA.");
  var images=new Array("amex","help","delta","cp","do","ou");
  Y.all("#hovers a").on("mouseover",function(e){
	  var nodelist = Y.all("#hovers a");
	  Y.all("#hovers a").each(function(no){
		  no.removeClass("selected");
	  });
	  var tag=e.target.get("tagName");
	  var node="";
	  if(tag=="IMG"){
		  node=e.target.ancestor().ancestor();
	  }else if(tag=="SPAN"){
		  node=e.target.ancestor();
	  }else{
		  node=e.target;
	  }
	  node.addClass("selected");
	  var index=nodelist.indexOf(node);
	  Y.one("#p_13872472-header-image img").set("src",images[index]+".png");
	  Y.one("#p_13872472-header-image img").set("alt",titles[index]);
	  Y.one("#p_13872472-header-image img").set("title",titles[index]);
	  Y.one(".x3-large").setHTML(titles[index]);
	  Y.one(".x3-large").set("href",urls[index]);
	  Y.one("#sent").setHTML(sentences[index]);
	  Y.one("#sent-link").set("href",urls[index]);
  });

  
 /*  for(var index=0;index<nodelist.size();index++){   
     nodelist.item(index).simulate('mouseover');  
   }  
*/
});
var i=0;
self.setInterval(function(){ i++; if(i>4){i=0;}clock(i);},5000);
function clock(i)
{
	YUI().use('node', 'event','node-event-simulate', function (Y) {
		 var nodelist = Y.all("#hovers a");
		nodelist.item(i).simulate('mouseover');
	});
}
